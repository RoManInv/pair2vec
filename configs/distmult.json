{
  "dataset_reader": {
    "type": "fb15k",
    "data_dir": "/fb15k-237/",
    "preprocessed_dir": "/fb15k-237/preprocessed"
  },
  "train_data_path": {
    "triples_file": "train.txt"
  },
  "validation_data_path": {
    "triples_file": "valid.txt"
  },
  "model": {
    "type": "distmult",
    "dropout": 0.2,
    "embedding_dim": 10
  },
  "iterator": {
        "type": "basic",
        "batch_size": 120
    },
  "trainer": {
    "num_epochs": 20,
    "grad_norm": 10.0,
    "patience": 10,
    "validation_metric": "+mrr",
    "cuda_device": 0,
    "learning_rate_scheduler": {
        "type": "reduce_on_plateau",
        "factor": 0.5,
        "mode": "max",
        "patience": 2
    },
    "optimizer": {
      "type": "adam",
      "betas": [0.9, 0.999],
      "lr": 0.001
    }
  }
}
