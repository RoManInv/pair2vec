{
    "dataset_reader": {
        "type": "squad2",
        "token_indexers": {
            "relemb_tokens": {
                "type": "single_id",
                "lowercase_tokens": true,
                "namespace": "relemb_tokens"
            },
            "elmo": {
                "type": "elmo_characters"
            }
        }
    },
    "train_data_path": "data/squad/train-v2.0.json",
    "validation_data_path": "data/squad/dev-v2.0.json",
    "vocabulary": {
        "directory_path": "models/relemb_vocabulary"
    },
    "model": {
        "type": "relemb-docqa-no-answer",
        "relemb_dropout": 0.15,
        "mask_key": "elmo",
        "ablation_type": "attn_over_rels",
        "config_file": "models/typed_2-2_normalized/relemb.json",
        "model_file": "models/typed_2-2_normalized/best.pt",
        "embedding_keys": [
            "elmo"
        ],
        "text_field_embedder": {
            "elmo": {
                "type": "elmo_token_embedder",
                "options_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_options.json",
                "weight_file": "https://s3-us-west-2.amazonaws.com/allennlp/models/elmo/2x4096_512_2048cnn_2xhighway/elmo_2x4096_512_2048cnn_2xhighway_weights.hdf5",
                "do_layer_norm": false,
                "dropout": 0.0
            }
        },
        "phrase_layer": {
            "type": "gru",
            "input_size": 1024,
            "hidden_size": 100,
            "num_layers": 1,
            "bidirectional": true
        },
        "residual_encoder": {
            "type": "gru",
            "input_size": 200,
            "hidden_size": 100,
            "num_layers": 1,
            "bidirectional": true
        },
        "no_answer_scorer": {
            "input_dim": 600,
            "num_layers": 2,
            "hidden_dims": [
                80,
                1
            ],
            "activations": [
                "relu",
                "linear"
            ],
            "dropout": [
                0.2,
                0.0
            ]
        },
        "span_start_encoder": {
            "type": "gru",
            "input_size": 200,
            "hidden_size": 100,
            "num_layers": 1,
            "bidirectional": true
        },
        "span_end_encoder": {
            "type": "gru",
            "input_size": 400,
            "hidden_size": 100,
            "num_layers": 1,
            "bidirectional": true
        }
    },
    "iterator": {
        "type": "bucket",
        "sorting_keys": [
            [
                "passage",
                "num_tokens"
            ],
            [
                "question",
                "num_tokens"
            ]
        ],
        "batch_size": 30
    },
    "trainer": {
        "num_epochs": 30,
        "grad_norm": 10.0,
        "patience": 5,
        "cuda_device": 0,
        "num_serialized_models_to_keep": 2,
        "validation_metric": "+em",
        "learning_rate_scheduler": {
            "type": "reduce_on_plateau",
            "factor": 0.5,
            "mode": "max",
            "patience": 1
        },
        "optimizer": {
            "type": "adam",
            "betas": [
                0.9,
                0.9
            ]
        }
    }
}